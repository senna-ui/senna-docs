{
  "name": "senna-docs",
  "private": true,
  "version": "1.0.0",
  "description": "The official Senna documentation",
  "author": "Stefan Zollinger",
  "license": "Apache-2.0",
  "scripts": {
    "start": "stencil build --dev --serve --watch",
    "prestart": "npm run -s build-all",
    "build": "stencil build --prerender",
    "build.ci": "npm run build-all && stencil build --prerender --ci",
    "prebuild": "npm run -s build-all",
    "lint": "eslint 'src/**/*.{ts,tsx,js}' 'scripts/**/*.{ts,tsx,js}'",
    "lint.fix": "eslint --fix 'src/**/*.{ts,tsx,js}' 'scripts/**/*.{ts,tsx,js}'",
    "prettier.fix": "prettier --write 'src/**/*.{ts,tsx,js,css}' 'scripts/**/*.{ts,tsx,js,css}'",
    "build-all": "ts-node -P scripts/tsconfig.json scripts/build-all",
    "build-data": "ts-node -P scripts/tsconfig.json scripts/build-data",
    "build-pages": "ts-node -P scripts/tsconfig.json scripts/build-pages",
    "watch-pages": "ts-node -P scripts/tsconfig.json scripts/watch-pages",
    "heroku-postbuild": "npm run build.ci"
  },
  "dependencies": {
    "@senna-ui/core": "0.1.0-dev.202012281518.eb60f14",
    "@senna-ui/docs": "0.1.0-dev.202012281518.eb60f14",
    "@sentry/node": "^5.17.0",
    "@stencil/router": "^1.0.1",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "helmet": "^3.22.0",
    "ionicons": "^5.1.2",
    "moment": "^2.24.0",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.1.5",
    "simple-git": "^2.6.0",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "@ionic/eslint-config": "^0.3.0",
    "@stencil/core": "1.14.0",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "8.1.0",
    "@types/listr": "0.14.2",
    "@types/marked": "0.7.4",
    "@types/node-fetch": "2.5.7",
    "@types/prettier": "^2.1.2",
    "@types/prismic-dom": "2.1.1",
    "@types/prismjs": "^1.9.1",
    "@types/semver": "7.2.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@typescript-eslint/types": "^4.4.1",
    "chokidar": "^3.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fast-glob": "^3.2.2",
    "front-matter": "^4.0.2",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.3",
    "listr": "^0.14.3",
    "marked": "^1.1.0",
    "node-fetch": "^2.6.1",
    "ora": "^4.0.3",
    "prettier": "^2.1.2",
    "prismjs": "~1.21.0",
    "react": "^16.13.1",
    "ts-node": "^8.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
